# A sample Gemfile
source "https://rubygems.org"

gem "thin"
gem "bootstrap-sass"
gem "compass"
gem "sinatra"
gem "sinatra-contrib"
gem "ne_api_sdk_ruby", github: "k2works/ne_api_sdk_ruby"

group :development, :test do
  gem 'pry'
  gem 'rspec'
end

group :test do
  gem 'rack-test'
  gem 'capybara'
  gem 'launchy'
  gem 'selenium-webdriver'
end